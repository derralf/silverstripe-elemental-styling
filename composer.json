{
    "name": "derralf/elemental-styling",
    "description": "An extension to add some styling option to Elements. Suggested/required for some of my silverstripe-elemental element modules",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "dnadesign/silverstripe-elemental": "^4 || ^5"
    }
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Derralf\\ElementalStyling\\": "src/"
        }
    }
}

